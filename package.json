{
  "name": "seize",
  "version": "0.2.0",
  "description": "Seize is light Node or Browser web-page content extractor inspired by arc90 readability and Safari Reader",
  "main": "seize.js",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "watch": "./node_modules/.bin/jest --watch",
    "lint": "./node_modules/eslint/bin/eslint.js *.js **/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peremenov/seize.git"
  },
  "author": [
    "Kir Peremenov <kirill@peremenov.com> (https://peremenov.com)"
  ],
  "license": "MIT",
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "3.17.1",
    "eslint-config-airbnb-base": "11.1.1",
    "eslint-plugin-import": "2.2.0",
    "jest": "20.0.0",
    "jsdom": "9.11.0",
    "lodash": "4.17.4",
    "sinon": "2.2.0"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/test/**/*.js"
    ]
  },
  "keywords": [
    "dom",
    "html",
    "extract",
    "extractor",
    "document",
    "text",
    "significant part",
    "content",
    "readability",
    "reader"
  ]
}
